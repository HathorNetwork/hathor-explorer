{
  "name": "hathor-admin",
  "version": "0.24.2",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "private": true,
  "dependencies": {
    "@hathor/wallet-lib": "2.8.2",
    "@reduxjs/toolkit": "2.5.1",
    "@unleash/proxy-client-react": "1.0.4",
    "axios": "1.8.2",
    "bootstrap": "5.3.3",
    "buffer": "6.0.3",
    "d3-selection": "3.0.0",
    "d3-zoom": "3.0.0",
    "font-awesome": "4.7.0",
    "highlight.js": "11.10.0",
    "jquery": "3.7.1",
    "lodash": "4.17.21",
    "node-stdlib-browser": "1.3.1",
    "npm-run-all": "4.1.5",
    "patch-package": "8.0.0",
    "popper.js": "1.16.1",
    "prop-types": "15.8.1",
    "qrcode.react": "3.1.0",
    "react": "18.2.0",
    "react-cookie-consent": "9.0.0",
    "react-copy-to-clipboard": "5.1.0",
    "react-dom": "18.2.0",
    "react-gtm-module": "2.0.11",
    "react-loading": "2.0.3",
    "react-paginate": "8.2.0",
    "react-redux": "9.2.0",
    "react-router-dom": "6.29.0",
    "react-scripts": "5.0.1",
    "sass": "1.77.8",
    "unleash-proxy-client": "1.11.0",
    "viz.js": "2.1.2"
  },
  "scripts": {
    "build-css": "sass src/index.scss:src/index.css && sass src/newUi.scss:src/newUi.css",
    "watch-css": "npm run build-css && sass --watch src:src",
    "start-js": "react-app-rewired --openssl-legacy-provider start",
    "start": "npm-run-all -p watch-css start-js",
    "build-js": "react-app-rewired --openssl-legacy-provider build",
    "build": "npm-run-all build-css build-js",
    "test": "react-app-rewired test --env=jsdom",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint:scss": "stylelint '**/*.scss'",
    "lint:scss:fix": "stylelint '**/*.scss' --fix",
    "format": "prettier --write 'src/**/*.js'",
    "format:check": "prettier --check 'src/**/*.js'",
    "eject": "react-scripts eject",
    "postinstall": "patch-package"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/eslint-parser": "7.26.8",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.35.0",
    "prettier": "1.19.1",
    "react-app-rewired": "2.2.1",
    "stylelint": "16.12.0",
    "stylelint-config-standard-scss": "14.0.0",
    "stylelint-scss": "6.10.0"
  },
  "overrides": {
    "react-refresh": "0.14.0"
  }
}
